<div class = "row">
    <div class = "col-md-12">
        <h2> Images </h2>
        <hr>
    </div>
    {{#each images}}
        <div class = "col-md-4">
            {{!--  <img src = "/images/uploaded/1539198357665.jpg" class="img-responsive img-thumbnail" /> --}}
           {{!-- {{this}} --}}
             <img src = "/images/uploaded/{{this}}" class = "img-responsive img-thumbnail"/>
        </div>  
    {{else}}
        <div class= "col-md-12 text-center">
            <strong> No images Available </strong>
        </div>
    {{/each}}
    
</div>